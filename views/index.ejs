<% include ./partials/header %>

<h1>This is the index</h1>

<% blog.forEach(function(blog) { %>
    <div>
        <h2><%=blog.title %></h2>
        <img src = "<%= blog.image %>"></img>
        <span><%= blog.created %></span>
        <p><%= blog.body %></p>
        <a href="/blogs/<%=blog._id %>">Read More</a>
    </div>
<% }) %>

<% include ./partials/footer %>